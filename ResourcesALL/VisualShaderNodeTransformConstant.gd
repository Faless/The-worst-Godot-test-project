extends Node2D

var q_VisualShaderNodeTransformConstant : VisualShaderNodeTransformConstant = VisualShaderNodeTransformConstant.new()
var counter : float
var C_COUNTER : Vector2 = Vector2(0.5,1.0)

func _ready():
	counter = randf() * (C_COUNTER.y - C_COUNTER.x) + C_COUNTER.x

